{
    "env": {
        "commonjs": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "rules": {
        "no-console": ["error", { "allow": ["info", "error", "warn"] }],
        "max-len": ["warn", { "code": 100, "ignoreComments": true, "ignoreStrings": true }],
        "no-param-reassign": ["error", { "props": false }]
    }
}
